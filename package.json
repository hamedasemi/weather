{
  "name": "cv-polymer",
  "version": "0.0.0-build.0",
  "description": "CV, a progressive web application.",
  "private": true,
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "release": "npm version prerelease -m 'Bumped to %s' --git-tag-version=true",
    "postrelease": "git push && git push --tags",
    "deploy": "echo deploy",
    "serve": "firebase serve --port=8080",
    "sync": "browser-sync start --port 8081 --proxy 'http://localhost:5000' --logLevel debug --files './src' --open 'external'",
    "clean": "rm -r dist",
    "gulp": "gulp --require babel-register --gulpfile=./gulp/gulp.js --cwd=./",
    "test": "mocha --compilers js:babel-register ./server/**/*.test.js"
  },
  "dependencies": {
    "firebase-tools": "^3.2.3"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-register": "^6.7.2",
    "browser-sync": "^2.18.5",
    "css-mqpacker": "^5.0.1",
    "gulp-polymer-babel": "^1.0.1",
    "gulp-postcss": "^6.2.0",
    "postcss-apply": "^0.4.0",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.1",
    "postcss-nested": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "webrew": "0.0.26",
    "webrew-helpers-log": "0.0.7",
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-root-import": "^4.1.5",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "del": "^2.2.0",
    "eslint": "^3.12.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-babel": "^6.1.2",
    "gulp-cli": "^1.2.1",
    "gulp-font": "^1.0.3",
    "gulp-newer": "^1.1.0",
    "gulp-polymer-postcss": "^1.0.0",
    "gulp-rename": "^1.2.2",
    "mocha": "^3.2.0",
    "stream-combiner2": "^1.1.1"
  },
  "repository": {
    "type": "git",
